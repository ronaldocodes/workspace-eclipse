package exercicios;

import java.util.Scanner;

public class exercicio004 {

	public static void main(String[] args) {

	Scanner sc = new Scanner(System.in);

	int password = 1234;
	int x = sc.nextInt();
	
	
	while( x != password) {
		System.out.println("Senha errada!");
		x = sc.nextInt();
	}
	
	System.out.println("Senha correta!");
	
	
	sc.close(); 
	}

}
